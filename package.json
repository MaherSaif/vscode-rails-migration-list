{
    "name": "rails-migration-list",
    "displayName": "rails-migration-list",
    "description": "List rails migrations files",
    "version": "0.1.0",
    "publisher": "MaherSaif",
    "engines": {
        "vscode": "^1.12.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MaherSaif/vscode-rails-migration-list"
    },
    "homepage": "https://github.com/MaherSaif/vscode-rails-migration-list",
    "galleryBanner": {
        "color": "#c52f24",
        "theme": "dark"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Ruby on Rails",
        "Migrations"
    ],
    "activationEvents": [
        "onCommand:extension.rails-migrations-list"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.rails-migrations-list",
                "title": "Rails Migrations List"
            }
        ],
        "keybindings": [
            {
                "command": "extension.rails-migrations-list",
                "key": "cmd+shift+m"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}